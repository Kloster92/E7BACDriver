BEGIN DISPLAY LAST SESSION
ANALYSIS AC_SWEEP
SYMBOL ALWAYS
TRACECOLORSCHEME NORMAL
BEGIN ANAPLOT 1
ACTIVE
XBASE
BEGIN XAXIS
XAXISUSERNAME 0 (null)
RANGEFLAG AUTO
TYPE LOG
UNIT H
BEGIN GRIDS
MAJORSPACINGLINEAR 16650
MAJORSPACINGLOG 1
MINORINTERVALS 10
MAJORNUMBERS
MAJORSTYLE LINES
MAJORPATTERN SOLID
MINORSTYLE LINES
MINORPATTERN DOT
END GRIDS
END XAXIS
BEGIN YAXIS 1
YAXISSIDE LEFT
RANGEFLAG AUTO
TYPE LINEAR
UNIT  
BEGIN GRIDS
AUTOMATIC
MAJORNUMBERS
MAJORSTYLE LINES
MAJORPATTERN SOLID
MINORSTYLE LINES
MINORPATTERN DOT
END GRIDS
TEXT Gain
BEGIN TRACE DB(V(Flyback:OFB)/V(Flyback:INJ))
MARKERID 48
END TRACE DB(V(Flyback:OFB)/V(Flyback:INJ))
END YAXIS 1
BEGIN YAXIS 2
YAXISSIDE RIGHT
ACTIVE
RANGEFLAG AUTO
TYPE LINEAR
UNIT d
BEGIN GRIDS
AUTOMATIC
MAJORNUMBERS
MAJORSTYLE LINES
MAJORPATTERN SOLID
MINORSTYLE LINES
MINORPATTERN DOT
END GRIDS
TEXT Phase
BEGIN TRACE P(V(Flyback:OFB)/ V(Flyback:INJ))
MARKERID 49
END TRACE P(V(Flyback:OFB)/ V(Flyback:INJ))
END YAXIS 2
END ANAPLOT 1
BEGIN MEASUREMENTS
SIZE 104
BEGIN MEASUREMENT 1
NAME GainMargin(Ptot-180,Gtot)
EVALUATE 1
END MEASUREMENT 1
BEGIN MEASUREMENT 2
NAME PhaseMargin(Gtot,Ptot-180)
EVALUATE 1
END MEASUREMENT 2
BEGIN MEASUREMENT 3
NAME ZeroCross(Gtot)
EVALUATE 1
END MEASUREMENT 3
BEGIN MEASUREMENT 4
NAME YatX(Gtot,65.5k)
EVALUATE 1
END MEASUREMENT 4
END MEASUREMENTS
END DISPLAY LAST SESSION
